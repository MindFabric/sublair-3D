{
  "name": "sketchbook",
  "version": "0.4.0",
  "description": "3D playground built on three.js and cannon.js",
  "keywords": [
    "3d",
    "threejs",
    "cannonjs",
    "game",
    "engine",
    "typescript",
    "sketchbook"
  ],
  "main": "build/sketchbook.min.js",
  "types": "build/types/sketchbook.d.ts",
  "scripts": {
    "dev": "concurrently \"set NODE_OPTIONS=--openssl-legacy-provider && webpack-dev-server --config webpack.dev.js\" \"node api/server.js\"",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && webpack --config webpack.prod.js",
    "start": "node api/server.js",
    "api": "node api/server.js",
    "api:dev": "nodemon api/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/swift502/Sketchbook.git"
  },
  "author": "swift502 <blaha.j502@gmail.com> (http://jblaha.art/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swift502/Sketchbook/issues"
  },
  "homepage": "https://github.com/swift502/Sketchbook",
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.168",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "concurrently": "^9.2.1",
    "css-loader": "^3.6.0",
    "style-loader": "^1.3.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.9",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "firebase-admin": "^12.7.0",
    "flv.js": "^1.6.2",
    "helmet": "^7.0.0",
    "jquery": "^3.6.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.7.0",
    "node-media-server": "^4.1.0",
    "nodemon": "^2.0.22",
    "sweetalert2": "^9.17.2",
    "three": "0.113.0",
    "three-csm": "1.0.2",
    "ws": "^8.18.3"
  }
}
